CREATE TABLE `Reservations` (
  `ReservationID` int NOT NULL,
  `Email` varchar(25) DEFAULT NULL,
  `RoomID` int DEFAULT NULL,
  `CheckInDate` datetime DEFAULT NULL,
  `CheckOutDate` datetime DEFAULT NULL,
  `AmenityID` varchar(45) DEFAULT NULL,
  `AttractionsID` varchar(45) DEFAULT NULL,
  `TotalGuests` int DEFAULT NULL,
  PRIMARY KEY (`ReservationID`),
  KEY `Email_idx` (`Email`),
  KEY `RoomID_idx` (`RoomID`),
  CONSTRAINT `RoomID` FOREIGN KEY (`RoomID`) REFERENCES `Rooms` (`RoomID`)
  CONSTRAINT `Email` FOREIGN KEY (`Email`) REFERENCES `User` (`Email`)
)
